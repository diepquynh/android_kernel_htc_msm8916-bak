&soc {
	i2c@78b6000 { /* BLSP1 QUP2 */
		qcom,fs-clk-div = <32>;
		qcom,high-time-clk-div = <16>;

		tca6418@34 {
			compatible = "ti,tca6418";
			reg = <0x34>;
			tca6418,reset_gpio = <&msm_gpio 51 0x00>;
		};

		htc_mcu@3a {
			compatible = "htc_mcu";
			reg = <0x3a>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <36 0x0>;
			hub_1v8-supply = <&ncp6924_ldo3>;
			mcu,intr-gpio  = <&msm_gpio 36 0x00>;
			mcu,GS_chip_layout = <1>;
			mcu,Acceleration_axes  = <0>;
			mcu,Magnetic_axes  = <7>;
			mcu,Gyro_axes  = <0>;
			mcu,Cpu_wake_mcu-gpio    = <&msm_gpio 3 0x00>;
			mcu,Reset-gpio    = <&msm_gpio 0 0x00>;
			mcu,Chip_mode-gpio    = <&msm_gpio 8 0x00>;
			mcu,ALS_goldh  = <0x17>;
			mcu,ALS_goldl  = <0x4D>;
		};

		g_sensor_kxtj2@e {
			compatible = "htc_g_sensor,kxtj2";
			reg = <0x0e>;
			interrupt-parent = <&msm_gpio>;
			interrupt = <98 0x0>;
			g_sensor_kxtj2,intr = <&msm_gpio 98 0x00>;
			g_sensor_kxtj2,placement = <4>;
		};

		compass_akm09911@c {
			compatible = "htc_compass,akm09911";
			reg = <0xc>;
			compass_akm09911,layout     = <7>;
			compass_akm09911,gpio_RSTN  = <&msm_gpio 121 0x00>;
		};

        lv52130@7C {
            compatible = "disp-lv52130";
            reg = <0x7C>;
            htc,lcm_p5v-gpio = <&msm_gpio 32 0>;
            htc,lcm_n5v-gpio = <&msm_gpio 116 0>;
        };
                cm36686@60 {
                       compatible = "CM36686";
                       reg = <0x60>;
                       interrupt-parent = <&msm_gpio>;
                       interrupts = <113 0x02>;
                       interrupt-names = "cm36686";
                       cm36686,irq-gpio = <&msm_gpio 113 0x00>;
			cm36686,levels = <12 26 55 172 342 2441 3790 7055 9664 65535>;
                       cm36686,golden_adc = <0x6F9>;
                       cm36686,cm36686_slave_address = <0x60>;
                       cm36686,ps1_thd_set = <0x15>;
                       cm36686,ps1_thd_no_cal = <0xAAA>;
                       cm36686,dynamical_threshold = <1>;
                       cm36686,dark_level = <3>;
                       cm36686,ps_duty = <0>;
                       cm36686,ps_pers = <1>;
                       cm36686,ps_it = <7>;
                       cm36686,ps_hd = <0>;
                       cm36686,ps_led_current = <2>;
                 };

	};

        i2c@78b8000 {
                tps_65132@7C {
                        compatible = "disp-tps-65132";
                        reg = <0x7C>;
                };

                lp5521@32 {
                        compatible = "LP5521-LED";
                        reg = <0x32>;
                        lp5521,lp5521_en = <&msm_gpio 77 0x00>;
                        lp5521,charging_en = <&msm_gpio 112 0x00>;
                        lp5521,num_leds = <1>;
                };

		tps61310@33 {
			compatible = "TPS61310_FLASHLIGHT";
			reg = <0x33>;
			tps61310,tps61310_strb0 = <&msm_gpio 17 0x00>;
			tps61310,tps61310_strb1 = <&msm_gpio 34 0x00>;
			tps61310,flash_duration_ms = <600>;
			tps61310,enable_FLT_1500mA = <0>;
			tps61310,led_count = <1>;
			tps61310,disable_tx_mask = <1>;
			/* TODO: read board_info to know if dual-flash is supported */
			/* htc,board_info-source = <&htc_board_info>; */

			tps61310_flash0: htc,tps61310_flash_0 {
				label = "flash";
				linux,name = "flash_0";
				linux,default-trigger =
					"flash0_trigger";
				htc,id = <0>;
				htc,default-state = "off";
				htc,max-current = <750>;
			};

			tps61310_flash1: htc,tps61310_flash_1 {
				label = "flash";
				linux,name = "flash_1";
				linux,default-trigger =
					"flash1_trigger";
				htc,id = <1>;
				htc,default-state = "off";
				htc,max-current = <750>;
			};

			tps61310_torch0: htc,tps61310_torch_0 {
				label = "torch";
				linux,name = "torch_0";
				linux,default-trigger =
					"torch0_trigger";
				htc,id = <0>;
				htc,default-state = "off";
				htc,max-current = <200>;
			};

			tps61310_torch1: htc,tps61310_torch_1 {
				label = "torch";
				linux,name = "torch_1";
				linux,default-trigger =
					"torch1_trigger";
				htc,id = <1>;
				htc,default-state = "off";
				htc,max-current = <200>;
			};
		};

		/* TODO:
		 * This is the node added for csir2289 which has difference
		 * default programming with the correct one. Please add it
		 * together with the original smb1360 device tree node in your file.
		 * I copied from another project with might has different
		 * configurations with your project, please notice you can
		 * change any of the following settings to follow your project, except:
		 * 1) the phandle name: smb1360_otg_supply_14, this has to be match
		 *         with the one new added in usb_otg driver;
		 * 2) compatible string: this has to be match with the C code change;
		 * 3) reg: the I2C client address which csir2289 used, you can change it to
		 *         your own if it's not;
		 * 4) It should be used with 10mohm configuration.
		 */
		smb1360_otg_supply_14: smb1360-chg-fg@14 {
			compatible = "qcom,smb1360-chg-fg-csir2289";
			reg = <0x14>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <62 8>;
			pinctrl-names = "default";
			pinctrl-0 = <&smb_int_default>;
			regulator-name = "smb1360_otg_vreg";

			/* battery-profile select properties */
			qcom,batt-profile-select;
			qcom,smb1360-vadc = <&pm8916_vadc>;

			qcom,chg-inhibit-disabled;
			qcom,otg-fet-present;
			qcom,otg-fet-enable-gpio = <&pm8916_gpios 3 0>;
			qcom,charging-timeout = <768>;
			qcom,recharge-thresh-mv = <200>;
			qcom,fg-cutoff-voltage-mv = <3500>;
			qcom,fg-voltage-empty-mv = <3000>;
			htc,fg-voltage-empty-re-check-mv = <3200>;
			qcom,fg-auto-recharge-soc = <97>;
			qcom,iterm-ma = <50>;
			htc,batt-full-criteria = <350>;
			htc,batt-eoc-criteria = <50>;
			qcom,otg-batt-curr-limit = <1500>;

			qcom,thermal-mitigation = <1500 700 600 0>;
			qcom,warm-bat-mv = <4100>;
			qcom,warm-bat-decidegc = <48>;
			qcom,cool-bat-decidegc = <0>;
			qcom,clamp-soc-voltage-mv = <3400>;
			qcom,max-fastchg-ma = <1500>;
		};

		smb1360_otg_supply: smb1360-chg-fg@70 {
			compatible = "qcom,smb1360-chg-fg";
			reg = <0x70>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <62 8>;
			pinctrl-names = "default";
			pinctrl-0 = <&smb_int_default>;
			regulator-name = "smb1360_otg_vreg";

			/* battery-profile select properties */
			qcom,batt-profile-select;
			qcom,smb1360-vadc = <&pm8916_vadc>;

			qcom,chg-inhibit-disabled;
			qcom,otg-fet-present;
			qcom,otg-fet-enable-gpio = <&pm8916_gpios 3 0>;
			qcom,charging-timeout = <768>;
			qcom,recharge-thresh-mv = <200>;
			qcom,fg-cutoff-voltage-mv = <3500>;
			qcom,fg-voltage-empty-mv = <3000>;
			htc,fg-voltage-empty-re-check-mv = <3200>;
			qcom,fg-auto-recharge-soc = <97>;
			qcom,iterm-ma = <50>;
			htc,batt-full-criteria = <350>;
			htc,batt-eoc-criteria = <50>;
			qcom,otg-batt-curr-limit = <1500>;

			qcom,thermal-mitigation = <1500 700 600 0>;
			qcom,warm-bat-mv = <4100>;
			qcom,warm-bat-decidegc = <48>;
			qcom,cool-bat-decidegc = <0>;
			qcom,clamp-soc-voltage-mv = <3400>;
			qcom,max-fastchg-ma = <1500>;
		};
        };

